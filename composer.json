{
  "name": "taito/linqinp",
  "authors": [
    {
      "name": "taito",
      "email": "hanshin.abingdon@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Linqinp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LinqinpTest\\": "test"
    }
